<%- include('partials/header') %>
<h1>Home</h1>
<p><%= homeContent %></p>
<% posts.forEach(function(post) { %>
    <div>
        <h1><%= post.title %></h1>
        <p class="non-class"><%= post.content %></p>
        <a href="/posts/<%=post._id%>">Read more</a>
    </div>
<% }) %>
<%- include('partials/footer') %>